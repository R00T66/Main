local Events = { }

local HttpService = game:GetService("HttpService")
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")

function Events:PlayerAdded()
   return Players.PlayerAdded
end

function Events:PlayerRemoved()
   return Players.PlayerRemoving
end

function Events:JSONEncode(String)
   return HttpService:JSONEncode(String)
end

function Events:JSONDecode(Table)
   return HttpService:JSONDecode(Table)
end

return Events
