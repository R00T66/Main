local Notifications = { }
local MessageBox = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/NotificationGUI/main/source.lua"))()

function Notifications:SendNotification(Title, Text)
   MessageBox.Show({Position = UDim2.new(0.5,0,0.5,0), Text = Title, Description = Text, MessageBoxIcon = "Question", MessageBoxButtons = "OK", Result = function(res)
      if (res == "OK") then
         return
      end
   end})
end

return Notifications
